{
  "name": "help-me-api",
  "version": "1.0.1",
  "description": "API para help desk",
  "main": "index.js",
  "type": "module",
  "license": "AGPL-3.0-or-later",
  "author": "Diego Ferreira L.G.Oliveira",
  "repository": {
    "type": "git",
    "url": "https://github.com/diego64/help-me.git"
  },
  "scripts": {
    "dev": "tsx watch --env-file=.env src/server.ts",
    "build": "tsup src/server.ts --format esm,cjs --dts",
    "start": "node --env-file=.env dist/server.js",
    "generate": "dotenv -e .env -- prisma generate",
    "migrate": "dotenv -e .env -- prisma migrate dev",
    "studio": "dotenv -e .env -- prisma studio",
    "seed": "tsx --env-file=.env prisma/seed.ts",
    "seed-big": "tsx --env-file=.env prisma/seed-big.ts",
    "prisma:reset": "dotenv -e .env -- prisma migrate reset --force",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest run --reporter=verbose src/__tests__/unit/",
    "test:setup": "docker compose -f docker-compose.yaml up -d postgresql_helpme_teste mongodb_helpme_teste && sleep 10 && dotenv -e .env.test -- prisma db push --accept-data-loss",
    "test:e2e": "pnpm test:setup && vitest run src/__tests__/e2e",
    "test:e2e:watch": "vitest src/__tests__/e2e",
    "test:teardown": "docker compose -f docker-compose.yaml stop postgresql_helpme_teste mongodb_helpme_teste",
    "test:clean": "docker compose -f docker-compose.yaml down postgresql_helpme_teste mongodb_helpme_teste -v",
    "extracao-de-rotas": "node --env-file=.env scripts/extracao-de-rotas.cjs",
    "test:carga": "pnpm run extracao-de-rotas && k6 run src/__tests__/performance/carga/carga.js",
    "test:soak": "pnpm run extracao-de-rotas && k6 run src/__tests__/performance/soak/soak.js",
    "test:spike": "pnpm run extracao-de-rotas && k6 run src/__tests__/performance/spike/spike.js",
    "test:stress": "pnpm run extracao-de-rotas && k6 run src/__tests__/performance/stress/stress.js",
    "diagnostico:env": "tsx --env-file=.env scripts/check-env.ts",
    "diagnostico:db": "tsx --env-file=.env scripts/diagnostico-db.ts",
    "diagnostico:api": "bash scripts/check-api.sh"
  },
  "keywords": [],
  "packageManager": "pnpm@10.28.0",
  "dependencies": {
    "@influxdata/influxdb-client": "^1.35.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "7.2.0",
    "@prisma/config": "^7.2.0",
    "connect-redis": "^9.0.0",
    "cross-env": "^10.1.0",
    "express": "^5.2.1",
    "express-session": "^1.18.2",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.3",
    "kafkajs": "^2.2.4",
    "mongodb": "^7.0.0",
    "mongoose": "^9.1.4",
    "ms": "^2.1.3",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.12",
    "pg": "^8.17.1",
    "redis": "^5.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongodb": "^4.0.7",
    "@types/ms": "^2.1.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.9",
    "@types/nodemailer": "^7.0.5",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@vitest/coverage-v8": "4.0.17",
    "@vitest/ui": "4.0.17",
    "autocannon": "^8.0.0",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "madge": "^8.0.0",
    "prisma": "7.2.0",
    "supertest": "^7.2.2",
    "ts-complexity": "^0.0.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "pnpm": {
    "overrides": {
      "jws": ">=3.2.3",
      "glob": ">=11.1.0",
      "cross-spawn": ">=7.0.5",
      "brace-expansion": ">=2.0.2",
      "mem@<4.0.0": ">=4.0.0",
      "d3-color@<3.1.0": ">=3.1.0",
      "yargs-parser@>=6.0.0 <13.1.2": ">=13.1.2",
      "got@<11.8.5": ">=11.8.5",
      "tough-cookie@<4.1.3": ">=4.1.3",
      "tmp@<=0.2.3": ">=0.2.4",
      "form-data@<2.5.4": ">=2.5.4"
    }
  }
}