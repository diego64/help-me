{
  "name": "help-me-api",
  "version": "1.0.0",
  "description": "API para help desk",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src/server.ts --format esm,cjs --dts",
    "start": "node dist/server.js",
    "generate": "prisma generate",
    "migrate": "prisma migrate dev",
    "studio": "prisma studio",
    "seed": "tsx prisma/seed.ts",
    "seed-big": "tsx prisma/seed-big.ts",
    "prisma:reset": "prisma migrate reset --force",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest run --reporter=verbose src/__tests__/unit/",
    "test:setup": "docker-compose -f docker-compose.yaml up -d postgresql_helpme_teste mongodb_helpme_teste && sleep 10 && pnpm exec dotenv -e .env.test -- npx prisma db push --accept-data-loss",
    "test:e2e": "pnpm test:setup && pnpm exec dotenv -e .env.test -- pnpm vitest run src/__tests__/e2e",
    "test:e2e:watch": "pnpm exec dotenv -e .env.test -- pnpm vitest src/__tests__/e2e",
    "test:teardown": "docker-compose -f docker-compose.yaml stop postgresql_helpme_teste mongodb_helpme_teste",
    "test:clean": "docker-compose -f docker-compose.yaml down postgresql_helpme_teste mongodb_helpme_teste -v",
    "extracao-de-rotas": "node scripts/extracao-de-rotas.cjs",
    "test:carga": "pnpm run extracao-de-rotas && k6 run src/__tests__/performance/carga/carga.js",
    "test:soak": "pnpm run extracao-de-rotas && k6 run src/__tests__/performance/soak/soak.js",
    "test:spike": "pnpm run extracao-de-rotas && k6 run src/__tests__/performance/spike/spike.js",
    "test:stress": "pnpm run extracao-de-rotas && k6 run src/__tests__/performance/stress/stress.js",
    "diagnostico:env": "tsx scripts/check-env.ts",
    "diagnostico:db": "tsx scripts/diagnostico-db.ts",
    "diagnostico:api": "bash scripts/check-api.sh"
  },
  "keywords": [],
  "author": "Diego Ferreira L.G.Oliveira",
  "license": "ISC",
  "packageManager": "pnpm@10.23.0",
  "dependencies": {
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "7.2.0",
    "@prisma/config": "^7.0.0",
    "@types/dotenv": "^8.2.3",
    "bcrypt": "^6.0.0",
    "connect-redis": "^9.0.0",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "mongoose": "^8.19.2",
    "ms": "^2.1.3",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.12",
    "pg": "^8.16.3",
    "redis": "^5.9.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/ms": "^2.1.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.7.1",
    "@types/nodemailer": "^7.0.3",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@vitest/coverage-v8": "4.0.6",
    "@vitest/ui": "4.0.6",
    "dotenv-cli": "^11.0.0",
    "prisma": "7.2.0",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.5.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.6"
  },
  "pnpm": {
    "overrides": {
      "jws": ">=3.2.3",
      "glob": ">=11.1.0",
      "cross-spawn": ">=7.0.5",
      "brace-expansion": ">=2.0.2"
    }
  }
}